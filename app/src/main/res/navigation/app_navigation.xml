<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_navigation"
    app:startDestination="@id/homeViewFragment">

    <fragment
        android:id="@+id/homeViewFragment"
        android:name="ie.setu.appstore.views.home.HomeViewFragment"
        android:label="HomeViewFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeViewFragment_to_appAddFragment"
            app:destination="@id/appAddFragment" />
    </fragment>
    <fragment
        android:id="@+id/appAddFragment"
        android:name="ie.setu.appstore.views.add.AppAddFragment"
        android:label="AppAddFragment" />
</navigation>